{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "description": "My extension description",
    "permissions": [
      "activeTab",
      "storage",
      "webRequest",
      "tabs"
    ],
    "background": {
      "service_worker": "extension/background.js"
    },
    "action": {
      "default_popup": "extension/popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["extension/content.js"]
      }
    ]
  }